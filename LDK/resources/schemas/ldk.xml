<ns:tables xmlns:ns="http://labkey.org/data/xml">
    <ns:table tableName="metrics" tableDbType="TABLE">
        <ns:javaCustomizer>org.labkey.ldk.query.DefaultTableCustomizer</ns:javaCustomizer>
        <ns:columns>
            <ns:column columnName="rowid"/>
            <ns:column columnName="category"/>
            <ns:column columnName="metric_name"/>
            <ns:column columnName="floatvalue1"/>
            <ns:column columnName="floatvalue2"/>
            <ns:column columnName="floatvalue3"/>
            <ns:column columnName="stringvalue1"/>
            <ns:column columnName="stringvalue2"/>
            <ns:column columnName="stringvalue3"/>
            <ns:column columnName="referrerurl"/>
            <ns:column columnName="browser"/>
            <ns:column columnName="platform"/>
            <ns:column columnName="container"/>
            <ns:column columnName="createdby"/>
            <ns:column columnName="created"/>
            <ns:column columnName="modifiedby"/>
            <ns:column columnName="modified"/>
        </ns:columns>
    </ns:table>
    <ns:table tableName="integers" tableDbType="TABLE">
        <ns:columns>
            <ns:column columnName="value"/>
        </ns:columns>
    </ns:table>
    <ns:table tableName="notificationrecipients" tableDbType="TABLE" hidden="true">
        <ns:tableTitle>Notification Recipients</ns:tableTitle>
        <ns:auditLogging>DETAILED</ns:auditLogging>
        <ns:importUrl></ns:importUrl>
        <ns:insertUrl></ns:insertUrl>
        <ns:updateUrl></ns:updateUrl>
        <ns:deleteUrl></ns:deleteUrl>
        <ns:pkColumnName>rowid</ns:pkColumnName>
        <ns:columns>
            <ns:column columnName="rowid">
                <ns:shownInInsertView>false</ns:shownInInsertView>
                <ns:isAutoInc>true</ns:isAutoInc>
                <ns:isKeyField>true</ns:isKeyField>
            </ns:column>
            <ns:column columnName="notificationtype">
                <ns:columnTitle>Notification Type</ns:columnTitle>
                <ns:nullable>true</ns:nullable>
            </ns:column>
            <ns:column columnName="recipient">
                <ns:columnTitle>Recipients</ns:columnTitle>
                <ns:fk>
                    <ns:fkDbSchema>core</ns:fkDbSchema>
                    <ns:fkTable>PrincipalsWithoutAdmin</ns:fkTable>
                    <ns:fkColumnName>userid</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="container">
                <ns:isUserEditable>false</ns:isUserEditable>
                <ns:isHidden>true</ns:isHidden>
                <ns:fk>
                    <ns:fkDbSchema>core</ns:fkDbSchema>
                    <ns:fkTable>containers</ns:fkTable>
                    <ns:fkColumnName>entityid</ns:fkColumnName>
                </ns:fk>
            </ns:column>
            <ns:column columnName="createdby">
                <ns:isUserEditable>false</ns:isUserEditable>
                <ns:nullable>true</ns:nullable>
                <ns:isHidden>true</ns:isHidden>
            </ns:column>
            <ns:column columnName="created">
                <ns:isUserEditable>false</ns:isUserEditable>
                <ns:nullable>true</ns:nullable>
                <ns:isHidden>true</ns:isHidden>
            </ns:column>
            <ns:column columnName="modifiedby">
                <ns:isUserEditable>false</ns:isUserEditable>
                <ns:nullable>true</ns:nullable>
                <ns:isHidden>true</ns:isHidden>
            </ns:column>
            <ns:column columnName="modified">
                <ns:isUserEditable>false</ns:isUserEditable>
                <ns:nullable>true</ns:nullable>
                <ns:isHidden>true</ns:isHidden>
            </ns:column>
        </ns:columns>
    </ns:table>
    <ns:table tableName="ldapSyncMap" tableDbType="TABLE" hidden="false">
        <ns:tableTitle>LDAP Sync Map</ns:tableTitle>
        <ns:importUrl></ns:importUrl>
        <ns:insertUrl></ns:insertUrl>
        <ns:updateUrl></ns:updateUrl>
        <ns:deleteUrl></ns:deleteUrl>
        <ns:pkColumnName>rowid</ns:pkColumnName>
        <ns:columns>
            <ns:column columnName="rowid">
                <ns:shownInInsertView>false</ns:shownInInsertView>
                <ns:isAutoInc>true</ns:isAutoInc>
            </ns:column>
            <ns:column columnName="provider"/>
            <ns:column columnName="sourceId"/>
            <ns:column columnName="labkeyId"/>
            <ns:column columnName="type"/>
            <ns:column columnName="created"/>
        </ns:columns>
    </ns:table>
    <ns:table tableName="daysOfWeek" tableDbType="TABLE" hidden="false">
        <ns:tableTitle>Days Of The Week</ns:tableTitle>
        <ns:importUrl></ns:importUrl>
        <ns:insertUrl></ns:insertUrl>
        <ns:updateUrl></ns:updateUrl>
        <ns:deleteUrl></ns:deleteUrl>
        <ns:pkColumnName>idx</ns:pkColumnName>
        <ns:columns>
            <ns:column columnName="idx"/>
            <ns:column columnName="name"/>
        </ns:columns>
    </ns:table>
</ns:tables>
